{
  "tabWidth": 2,
  "trailingComma": "es5",
  "semi": true,
  "singleQuote": true,
  "useTabs": false,
  "parser": "typescript",
  "arrowParens": "always",
  "overrides": [
    {
      "files": "*.vue",
      "options": {
        "parser": "vue"
      }
    }
  ],
  "plugins": ["prettier-plugin-vue"]
}
