{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host --https --ssl-cert ./localhttps_cert.pem --ssl-key ./localhttps_key.pem",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "eval $(grep '^NUXT_HOST' .env) $(grep '^NUXT_PORT' .env) node .output/server/index.mjs",
    "format": "prettier --write **/*.{js,ts,vue}"
  },
  "dependencies": {
    "@morev/vue-transitions": "^2.3.6",
    "@pinia/nuxt": "^0.5.3",
    "@supabase/gotrue-js": "^2.65.0",
    "@supabase/supabase-js": "^2.42.5",
    "@vue/compiler-sfc": "^3.5.10",
    "chart.js": "^4.4.4",
    "estree-walker": "^3.0.3",
    "nuxt": "^3.11.2",
    "nuxt-snackbar": "^1.1.2",
    "pinia": "^2.2.1",
    "supabase": "^1.163.2",
    "swiper": "^11.1.10",
    "vue": "^3.5.10",
    "vue-chartjs": "^5.3.1",
    "vue-router": "^4.3.0",
    "vue3-snackbar": "^2.3.4"
  },
  "devDependencies": {
    "@element-plus/nuxt": "^1.1.0",
    "@nuxtjs/supabase": "^1.2.1",
    "prettier-plugin-vue": "^1.1.6",
    "typescript": "^5.7.2"
  }
}
